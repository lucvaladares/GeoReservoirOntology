    <AnnotationAssertion>
        <AnnotationProperty IRI="http://purl.obolibrary.org/obo/IAO_0000115"/>
        <IRI>#UFRGS:GeoReservoirOntology_channel_geometry</IRI>
        <Literal xml:lang="en">A synonym for Geometry with “channel” value</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#UFRGS:GeoReservoirOntology_channel_geometry</IRI>
