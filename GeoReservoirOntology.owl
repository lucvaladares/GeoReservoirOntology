    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty IRI="http://purl.obolibrary.org/obo/IAO_0000115"/>
        <IRI>#UFRGS:GeoReservoirOntology_channel_surface</IRI>
        <Literal xml:lang="en">A Geological Boundary that forms the basis of a Channel Unit, having a concave-up shape unless truncated by overlying depositional units.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#UFRGS:GeoReservoirOntology_channel_surface</IRI>
        <Literal xml:lang="en">channel surface</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty IRI="#UFRGS:GeoReservoirOntology_metaproperties"/>
